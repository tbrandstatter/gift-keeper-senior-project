<% provide(:title, 'Giftboard') %>
<h1>Create Giftboard</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@giftboard) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>


      <%= f.submit "Create Giftboard", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>