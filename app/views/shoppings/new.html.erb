<body class="subtle" >
<% provide(:title, 'Shopping') %>
<h1 id="giftlist">New Shopping Link</h1>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @shopping, :url => newshopping_path, :html => {:multipart => true} do |f| %>

     <p>
      <%= f.label :link %>
      <%= f.text_area :content, class: 'form-control' %>
    </p>

      <%= f.submit "Add Link", class: "btn btn-unique" %>
    <% end %>
  </div>
</div>