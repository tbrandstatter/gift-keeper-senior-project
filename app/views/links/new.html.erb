<body class="subtle" >
<% provide(:title, 'Links') %>
<h1 id="giftlist">Add Shopping Link</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:link, url: newlink_path) do |f| %>

      <%= f.label :link %>
      <%= f.text_field :content, class: 'form-control' %>

      <%= f.submit "Add Link", class: "btn btn-unique" %>
    <% end %>
  </div>
</div>